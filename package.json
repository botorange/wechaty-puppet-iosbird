{
  "name": "wechaty-puppet-iosbird",
  "version": "0.0.9",
  "description": "Puppet Iosbird for Wechaty",
  "directories": {
    "test": "tests"
  },
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "engines": {
    "wechaty": ">=0.17.46"
  },
  "wechaty": {
    "DEFAULT_HEAD": 0,
    "DEFAULT_PORT": 8080,
    "DEFAULT_PROTOCOL": "io|0.0.1",
    "DEFAULT_TOKEN": "WECHATY_IO_TOKEN",
    "DEFAULT_APIHOST": "api.chatie.io"
  },
  "scripts": {
    "start": "ts-node example/starter-bot.ts",
    "clean": "shx rm -fr dist/*",
    "dist": "npm run clean && tsc",
    "pack": "npm pack",
    "lint": "npm run lint:ts && npm run lint:md",
    "lint:md": "markdownlint README.md",
    "lint:ts": "tslint --project tsconfig.json && tsc --noEmit",
    "test": "npm run lint && npm run test:unit",
    "test:pack": "bash -x scripts/npm-pack-testing.sh",
    "test:unit": "blue-tape -r ts-node/register \"src/**/*.spec.ts\" \"src/*.spec.ts\" \"tests/*.spec.ts\" \"tests/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/botorange/wechaty-puppet-iosbird.git"
  },
  "keywords": [
    "chatie",
    "wechaty",
    "chatbot",
    "bot",
    "wechat",
    "sdk",
    "puppet",
    "iosbird"
  ],
  "author": "Rui LI <grace@botorange.come>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/botorange/wechaty-puppet-iosbird/issues"
  },
  "homepage": "https://github.com/botorange/wechaty-puppet-iosbird#readme",
  "devDependencies": {
    "@types/blue-tape": "^0.1.31",
    "@types/fs-extra": "^5.0.5",
    "@types/lru-cache": "^4.1.1",
    "@types/node": "^10.3.6",
    "@types/normalize-package-data": "^2.4.0",
    "@types/promise-retry": "^1.1.2",
    "@types/qr-image": "^3.2.1",
    "@types/quick-lru": "^1.1.0",
    "@types/read-pkg-up": "^3.0.1",
    "@types/semver": "^5.5.0",
    "blue-tape": "^1.0.0",
    "file-box": "^0.8.22",
    "git-scripts": "^0.2.1",
    "markdownlint-cli": "^0.13.0",
    "memory-card": "^0.4.10",
    "read-pkg-up": "^4.0.0",
    "semver": "^5.5.0",
    "shx": "^0.3.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.1.0",
    "typescript": "^2.9.2"
  },
  "git": {
    "scripts": {
      "pre-push": "./scripts/pre-push.sh"
    }
  },
  "peerDependencies": {
    "file-box": "^0.8.22",
    "memory-card": "^0.4.10",
    "wechaty-puppet": "^0.15.1"
  },
  "dependencies": {
    "@types/ws": "^6.0.1",
    "array-flatten": "^2.1.2",
    "brolog": "^1.6.5",
    "deferred-leveldown": "^5.0.0",
    "flash-store": "^0.6.6",
    "fs-extra": "^7.0.1",
    "hot-import": "^0.2.1",
    "lru-cache": "^5.1.1",
    "normalize-package-data": "^2.4.0",
    "p-queue": "^5.0.0",
    "portfinder": "^1.0.20",
    "promise-retry": "^1.1.1",
    "qr-image": "^3.2.0",
    "quick-lru": "^1.1.0",
    "state-switch": "^0.6.2",
    "uuidv4": "^2.0.0",
    "watchdog": "^0.8.10",
    "wechaty": "^0.22.6",
    "wechaty-puppet": "^0.15.1",
    "ws": "^6.1.2"
  }
}
